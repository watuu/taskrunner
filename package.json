{
  "name": "",
  "author": "watuu",
  "description": "",
  "license": "",
  "engines": {
    "node": "14.x"
  },
  "config": {
    "path_dest": "dest",
    "pug_ext": "html",
    "path_wp": "dest/wp-content/themes/xxx"
  },
  "scripts": {
    "css": "node-sass src/sass -o ${npm_package_config_path_dest}/assets/css --importer node_modules/node-sass-globbing/index.js --output-style expanded && postcss ${npm_package_config_path_dest}/assets/css/*.css -o ${npm_package_config_path_dest}/assets/css/*.css --no-map --use autoprefixer && csscomb ${npm_package_config_path_dest}/assets/css/*.css",
    "css-watch": "chokidar src/sass/ -c 'npm run css'",
    "pug": "pug src/pug/ --hierarchy -o ${npm_package_config_path_dest} -P '\t' -E ${npm_package_config_pug_ext}",
    "pug-watch": "pug src/pug/ --hierarchy -o ${npm_package_config_path_dest} -w -P '\t'  -E ${npm_package_config_pug_ext}",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch",
    "watch": "npm run css-watch & npm run pug-watch & npm run webpack-watch"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "chokidar-cli": "^2.1.0",
    "css-loader": "^5.0.1",
    "csscomb": "^4.3.0",
    "node-sass": "^4.14.1",
    "node-sass-globbing": "0.0.23",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "pug": "^3.0.0",
    "pug-cli": "git+https://github.com/watuu/pug-cli.git",
    "style-loader": "^2.0.0",
    "webpack": "^4.45.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "gsap": "^3.5.1",
    "jquery": "^3.5.1",
    "jquery-jpostal-ja": "^2.14.4",
    "object-fit-images": "^3.2.4",
    "slick-carousel": "^1.8.1"
  }
}
